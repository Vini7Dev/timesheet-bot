generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Users {
  id          String    @id @default(uuid())
  name        String
  email       String    @unique
  username    String    @unique
  password    String
  created_at  DateTime  @default(now())
  updated_at  DateTime  @default(now())

  @@map("users")
}
